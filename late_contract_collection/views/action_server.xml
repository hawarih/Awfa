<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="add_generalization_action" model="ir.actions.server">
            <field name="name">Add Generalization</field>
            <field name="model_id" ref="customer_rental.model_rental_contract"/>
            <field name="state">code</field>
            <field name="code">action = records.add_generalization()</field>
        </record>

        <record id="add_statement_action1" model="ir.actions.server">
            <field name="name">Add Statement</field>
            <field name="model_id" ref="customer_rental.model_rental_contract"/>
