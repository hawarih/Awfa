            <field name="code">action = records.add_statement()</field>
        </record>

        <record id="cancel_arrest_warrant_action" model="ir.actions.server">
            <field name="name">Cancel Arrest Warrant</field>
            <field name="model_id" ref="customer_rental.model_rental_contract"/>
