            <field name="code">action = records.cancel_arrest_warrant()</field>
        </record>

        <record id="cancel_generalization_action" model="ir.actions.server">
            <field name="name">Cancel Generalization</field>
            <field name="model_id" ref="customer_rental.model_rental_contract"/>
            <field name="state">code</field>
            <field name="code">action = records.cancel_generalization()</field>
        </record>

        <record id="transfer_to_generalization_action" model="ir.actions.server">
            <field name="name">Transfer To Generalization</field>
            <field name="model_id" ref="customer_rental.model_rental_contract"/>
            <field name="state">code</field>
            <field name="code">action = records.transfer_to_generalization()</field>
        </record>
    </data>
</odoo>