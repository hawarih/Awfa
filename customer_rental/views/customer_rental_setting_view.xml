<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="customer_rental_setting_form" model="ir.ui.view">
            <field name="name">customer.rental.setting.form</field>
            <field name="model">customer.rental.setting</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record id="customer_rental_setting_tree" model="ir.ui.view">
            <field name="name">customer.rental.setting.tree</field>
            <field name="model">customer.rental.setting</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>